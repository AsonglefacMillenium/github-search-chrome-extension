{
  "name": "GitHub Search",
  "version": "1.1",
  "description": "Search GitHub repos",
  "homepage_url": "https://github.com/ushis/github-search-chrome-extension",
  "update_url": "https://github.com/ushis/github-search-chrome-extension/raw/master/updates.xml",
  "icons": {
    "16": "icons/16.ico",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "omnibox": {
    "keyword": "gh"
  },
  "permissions": [
    "https://api.github.com/"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "jquery-localstorage-cache.js",
      "github_search.js"
    ]
  }
}
